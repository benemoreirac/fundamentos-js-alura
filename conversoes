//tipo de dado
//booleanos

//conversao implicita
const numero = 456;
const numeroString = "456";

console.log(numero === numeroString) //compara valor e tipo
console.log(numero == numeroString) //compara apenas o que "esta dentro"
console.log(numero + numeroString) //concatena ao inves de somar

//conversao explicita
console.log(numero + Number(numeroString))
